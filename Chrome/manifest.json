{
  "manifest_version": 2,

  "name": "Candy Crush Crack",
  "description": "Unlock level without hassle",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "*://candycrush.king.com/",
    "tabs",
    "webRequest"
  ]
}
